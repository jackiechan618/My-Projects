    <div>
        <span style="font-size:16px; font-weight:bold;">Welcome: <%= session.user.name %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<span>
        <a class="btn btn-danger btn-large" href="/userPasswordModify">Modify Password</a>
